# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure("2") do |config|
  config.vm.define "Serveur-AD" do |ad|
    ad.vm.box = "Vagrant_Project_M2I/Windows_Serveur"
    ad.vm.network "private_network", ip: "192.168.56.11"
    
  end
  
  config.vm.provider "virtualbox" do |hw|
    hw.cpus = 4
    hw.memory = "4096"
  end
end

